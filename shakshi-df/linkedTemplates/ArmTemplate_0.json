					"scriptLines": [
						"source(output(",
						"          order_id as string,",
						"          order_date as string,",
						"          customer_id as string,",
						"          product as string,",
						"          category as string,",
						"          region as string,",
						"          quantity as string,",
						"          price as string,",
						"          total_amount as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> source1",
						"source1 derive(quantity = toInteger(quantity),",
						"          price = toDouble(price),",
						"          total_amount = toInteger(quantity) * toDouble(price)) ~> derivedColumn1",
						"derivedColumn1 keyGenerate(output({type 1} as long),",
						"     startAt: 1L,",
						"     stepValue: 1L) ~> surrogateKey1",
						"surrogateKey1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> sink1"
